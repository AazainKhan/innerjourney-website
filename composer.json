{
    "name": "innerjourney/website",
    "description": "Inner Journey with Shanila - Coaching Website",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "autoload": {
        "psr-4": {
            "InnerJourney\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "8.4.0"
        }
    },
    "scripts": {
        "analyse": "phpstan analyse src --level=8",
        "cs-fix": "php-cs-fixer fix src --rules=@PSR12"
    }
}
